<% case subset_operator[:type]
when "RECENT" %>
  MOST RECENT
<% when "TIMEDIFF" %>
  Difference between times
<% when "DATEDIFF" %>
  Difference between dates
<% when "DATETIMEDIFF" %>
  Difference between date/times
<% else %>
  <%= subset_operator[:type] %>
<% end %>

<% if subset_operator[:value] && subset_operator[:value][:type]!= 'ANYNonNull' %>
  <%= render partial: 'highlighting/value', locals: {value: subset_operator[:value], range_comparison: "", tag: "span"} %>
<% end %>
