<% unless value[:type] == 'ANYNonNull' %>
  <% if value[:type] == 'PQ' ||value[:type] == 'TS'%>
    <%= rangeComparison %>
    <% if value[:inclusive?] %>=<% end %>
    <% if value[:type] == 'TS' %>
      <%=Date.strptime(value[:value], '%Y%m%d').strftime('%F')%>
    <% else %>
      <%= value[:value] %>
    <% end %>
    <% if value[:unit] %>
      <% unit_map = {
        'a'=>'year',
        'mo'=>'month',
        'wk'=>'week',
        'd'=>'day',
        'h'=>'hour',
        'min'=>'minute',
        's'=>'second'} %>
        <% if unit_map[value[:unit]] %>
          <%= unit_map[value[:unit]].pluralize(value[:value]) %>
        <% else %>
          <%= value[:unit] %>
        <% end %>
    <% end %>

  <% else %>
    <% if value[:type] == 'IVL_PQ' ||value[:type] == 'IVL_TS'%>
      <% if value[:high] && value[:low] %>
        <% if value[:high][:value] == value[:low][:value] && value[:high][:inclusive?] && value[:low][:inclusive?]%>
          <%= render partial: 'value', locals: {value:value[:low], rangeComparison: "", tag: "span"} %>
        <% else %>
          <%= render partial: 'value', locals: {value:value[:low], rangeComparison: ">", tag: "span"} %> and
          <%= render partial: 'value', locals: {value:value[:high], rangeComparison: "<", tag: "span"} %>
        <% end %>
      <% else %>
        <% if value[:high] %>
          <%= render partial: 'value', locals: {value:value[:high], rangeComparison: "<", tag: "span"} %>
        <% else %>
          <% if value[:low] %>
            <%= render partial: 'value', locals: {value:value[:low], rangeComparison: ">", tag: "span"} %>
          <% end %>
        <% end %>
      <% end %>
    <% else %>
      <% if value[:type] == 'CD' %>
        :
        <% valueSets = HealthDataStandards::SVS::ValueSet.where(oid: value[:code_list_id]) %>
        <% valueSets.each do |set| %>
          <%= set[:display_name] %>
        <% end %>
      <% end %>
    <% end %>
  <% end %>
<% end %>
